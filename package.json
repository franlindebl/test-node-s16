{
  "name": "s12-typescript-api",
  "version": "1.0.0",
  "description": "Node api with typescript, express and mongo",
  "main": "index.ts",
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "start": "ts-node-dev --inspect -- ./src/index.ts",
    "seed:team-and-player": "ts-node-dev ./src/seeds/team-and-player.seed.ts --ignore-watch",
    "start:pro": "npm run build && node ./dist/src/index.js",
    "build": "tsc",
    "build:swagger-json": "ts-node-dev ./src/utils/generate-swagger-json.ts --ingore-watch",
    "test": "cross-env DB_NAME=NODE-S16-TESTING-DB jest --silent",
    "prepare": "husky install"
  },
  "keywords": [
    "typescript",
    "node",
    "mongo",
    "express"
  ],
  "author": "Fran Linde",
  "license": "ISC",
  "dependencies": {
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^7.2.0",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.3.2",
    "reflect-metadata": "^0.1.13",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^4.6.3",
    "typeorm": "^0.3.16",
    "validator": "^13.9.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/mongoose": "^5.11.97",
    "@types/multer": "^1.4.7",
    "@types/node": "^20.2.3",
    "@types/supertest": "^2.0.12",
    "@types/validator": "^13.7.17",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "cross-env": "^7.0.3",
    "eslint": "^8.42.0",
    "eslint-config-standard-with-typescript": "^35.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^8.0.0",
    "jest": "^29.5.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "5.0.4"
  }
}
